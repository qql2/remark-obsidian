{
  "name": "@qql2/remark-obsidian",
  "version": "1.0.2",
  "description": "Parse md with obsidian format to mdast.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -skipLibCheck && mv remark-obsidian.d.ts main.d.ts && node esbuild.config.mjs production",
    "pub": "npm run build & npm publish",
    "test": "tsx test/test-runner.ts"
  },
  "author": "qql2",
  "types": "main.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "remark": "^15.0.0",
    "unified": "^11.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/unist": "^3.0.0",
    "esbuild": "0.21.5",
    "tsx": "^4.0.0",
    "typescript": "^5.9.3"
  }
}